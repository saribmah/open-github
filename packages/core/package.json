{
  "name": "@open-github/core",
  "version": "1.0.0",
  "description": "Backend core package",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types.ts",
    "./config": "./src/config/config.ts",
